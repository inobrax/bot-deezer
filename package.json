    "npm": "4.2.0"
  },
  "dependencies": {
    "async": "^2.1.5",
    "body-parser": "^1.17.1",
    "bootstrap": "^3.3.7",
    "crypto": "0.0.3",
    "deezer-node-api": "^0.2.2",
    "es6-promisify": "^5.0.0",
